{
  "name": "manikin-mongodb",
  "description": "Data model abstractions for MongoDB",
  "version": "0.7.2",
  "main": "./lib/manikin.js",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "engines": {
    "node": "*"
  },
  "scripts": {
    "pretest": "coffee -c -o lib src",
    "prepublish": "coffee -c -o lib src",
    "test": "mocha -r should --compilers coffee:coffee-script spec/*.coffee",
    "coverage": "sh scripts/coverage.sh 'spec/mocha.js' all.html && scripts/coverage.sh 'spec/unit/*.coffee' unit.html",
    "prestart": "coffee -c -o lib src"
  },
  "dependencies": {
    "mongoose": "3.3.1",
    "mongojs": "0.4.5",
    "underscore": "1.4.2",
    "async": "0.1.22"
  },
  "devDependencies": {
    "coffee-script": "1.4.0",
    "should": "1.1.0"
  }
}