{
  "name": "manikin-mongodb",
  "description": "Data model abstractions for MongoDB",
  "version": "0.12.2",
  "main": "./lib/manikin-mongo.js",
  "author": "Jakob Mattsson <jakob.mattsson@gmail.com> (jakobmattsson.se)",
  "repository": {
    "type": "git",
    "url": "http://github.com/jakobmattsson/manikin-mongodb.git"
  },
  "engines": {
    "node": ">= 0.8.x < 0.12"
  },
  "scripts": {
    "pretest": "coffee -co lib src",
    "prepublish": "coffee -co lib src",
    "test": "mocha --compilers coffee:coffee-script spec/*.coffee",
