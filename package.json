    "prestart": "coffee -co lib src"
  },
  "dependencies": {
    "manikin-tools": ">= 0.1.1 < 0.2",
    "mongoose": "3.4.0",
    "underscore": ">= 1.4.0 < 2",
    "async": ">= 0.1.0 < 3"
  },
  "devDependencies": {
    "jscov": ">= 0.5.1 < 0.6",
    "mocha-term-cov-reporter": ">= 0.1.3 < 0.2",
    "mongojs": ">= 0.7.9 < 0.8",
    "mocha": ">= 1.7.4 < 2",
    "coffee-script": ">= 1.4.0 < 2",
    "should": ">= 1.2.1 < 2"
  }
}
